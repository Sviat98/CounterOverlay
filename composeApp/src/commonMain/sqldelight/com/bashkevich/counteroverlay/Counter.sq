import kotlin.Boolean;

CREATE TABLE CounterEntity (
    id TEXT NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    amount INTEGER NOT NULL
);

insertCounter:
INSERT OR REPLACE INTO CounterEntity(id, name, amount)
VALUES (?, ?, ?);

getAllCounters:
SELECT *
FROM CounterEntity;

deleteCounterById:
DELETE FROM CounterEntity
WHERE id = ?;

deleteAllCounters:
DELETE FROM CounterEntity;
